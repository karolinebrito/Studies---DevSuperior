package main;

import java.util.Scanner;

public class Exercises_Topic04_Aula07 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.println("n = ");
		int n = sc.nextInt();
		
		double vec[] = new double[n];
		
		for (int i=0; i<3; i++) {
			System.out.println("Value " + i+1);
			vec[i] = sc.nextDouble();
		}
				
		sc.close();

	}

}
