
public class main {

	public static void main(String[] args) {
		
		String product1, product2, gender;
		Double price1, price2;
		int age, code;
		
		product1 = "Computador";
		product2 = "TV";
		price1 = 2100.5;
		price2 = 1830.0;
		age = 30;
		code = 5291;
		gender = "F";
		
		
		
		System.out.printf("The product %c costs R$ %.2f", product1, price1);

		

		System.out.println("The product " + product2 + " costs R$ " + price2 + ".");
		System.out.println("");
		System.out.println("Code = " + code);
		System.out.println("");
		System.out.println("Personal Data: gender " + gender + " and age " + age + ".");
		
		
		
	}

}
